{{#each this}}
    <div class="photo-card">
        <a class="photo-link" href="{{largeImageURL}}">
            <img class="photo-img" src="{{webformatURL}}" alt="{{tags}}" loading="lazy" width="250" height="250"/>
        </a>
        <div class="info">
            <p class="info-item">
                <b>Likes</b><span>{{likes}}</span>
            </p>
            <p class="info-item">
                <b>Views</b><span>{{views}}</span>
            </p>
            <p class="info-item">
                <b>Comments</b><span>{{comments}}</span>
            </p>
            <p class="info-item">
                <b>Downloads</b><span>{{downloads}}</span>
            </p>
        </div>
    </div>
{{/each}}
